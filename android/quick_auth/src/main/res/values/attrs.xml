<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MQASignInButton">
        <attr name="button_type" format="integer">
            <enum name="standard" value="0" />
            <enum name="icon" value="1" />
        </attr>
        <attr name="button_theme" format="integer">
            <enum name="filled_black" value="0" />
            <enum name="filled_blue" value="1" />
            <enum name="outline" value="2" />
        </attr>
        <attr name="button_size" format="integer">
            <enum name="large" value="0" />
            <enum name="medium" value="1" />
            <enum name="small" value="2" />
        </attr>
        <attr name="button_text" format="integer">
            <enum name="signin_with" value="0" />
            <enum name="signup_with" value="1" />
        </attr>
        <attr name="button_shape" format="integer">
            <enum name="rectangular" value="0" />
            <enum name="pill" value="1" />
            <enum name="rounded" value="2" />
        </attr>
        <attr name="button_logo_alignment" format="integer">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="icon_left_text_center" value="2" />
        </attr>
    </declare-styleable>

</resources>